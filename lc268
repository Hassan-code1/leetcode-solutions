class Solution {
public:
    int missingNumber(vector<int>& nums) {
        vector<int> hash(nums.size()+1);
        for(auto& i: nums){
            hash[i]++;
        }
        for(int i = 0; i <= nums.size(); i++){
            if(hash[i] == 0){
                return i;
            }
        }
        return -1;
    }
};
