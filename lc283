class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int currPtr = 0;
        for(int i = 0; i < nums.size(); i++){
            if(nums[i] == 0) continue;
            nums[currPtr] = nums[i];
            currPtr++;
        }
        for(int i = currPtr; i < nums.size(); i++){
            nums[i] = 0;
        }
    }
};
